import type { ExecutorContext } from '@nrwl/devkit';
import { Target } from '@nrwl/devkit';
import { Observable } from 'rxjs';
export declare function runPostTargets({ postTargets, templateStringContext, context, projectName, }: {
    postTargets: string[];
    templateStringContext: Record<string, unknown>;
    context: ExecutorContext;
    projectName: string;
}): Observable<void>;
export declare function _getTargetOptions({ options, context, }: {
    options?: Record<string, unknown>;
    context: Record<string, unknown>;
}): Record<string, unknown>;
export declare function _checkTargetExist(target: Target, context: ExecutorContext): void;
