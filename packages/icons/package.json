{
    "name": "@web3uikit/icons",
    "version": "0.1.0",
    "sideEffects": false,
    "files": [
        "package.json",
        "dist"
    ],
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.cjs.js",
            "import": "./dist/index.es.js"
        }
    },
    "scripts": {
        "build": "vite build",
